        "intervention/image": "^2.4",
        "knplabs/github-api": "^2.8",
        "league/csv": "^9.1",
        "nesbot/carbon": "^2.10.0",
        "nikic/php-parser": "^4.0",
        "php-amqplib/php-amqplib": "^2.8",
        "php-http/guzzle6-adapter": "^1.1",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.1",
        "sentry/sentry": "^1.10",
        "symfony/console": "4.2.2",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.2",
        "symfony/framework-bundle": "4.2.2",
        "symfony/orm-pack": "^1.0",
        "symfony/test-pack": "^1.0",
        "symfony/twig-bundle": "^4.2",
        "symfony/validator": "^4.2",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/yaml": "^4.2",
        "toin0u/digitalocean-v2": "^2.2",
        "wildbit/postmark-php": "^2.6",
        "wohali/oauth2-discord-new": "^1.0",
        "xivapi/companion-php": "0.1.8",
        "xivapi/lodestone-parser": "1.8.18"
    },
    "require-dev": {
        "symfony/dotenv": "^4.2",
        "symfony/maker-bundle": "^1.5",
        "symfony/phpunit-bridge": "^4.2",
        "symfony/profiler-pack": "^1.0",
        "symfony/web-server-bundle": "^4.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C8JVTA2JD8H3V12QCEQW4DCD",
            "allow-contrib": false
        }
    }
